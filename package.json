{
  "name": "dynamodb-ts-model",
  "version": "1.1.3",
  "description": "A DynamoDB model/client with full TypeScript typings",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/henhal/dynamodb-model.git"
  },
  "author": "Henrik Hall <henrik.hall2@gmail.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.23.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.56.0",
    "@aws-sdk/lib-dynamodb": "^3.56.0",
    "dynamodb-expressions": "^2.3.2"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist && rm -f tsconfig.tsbuildinfo",
